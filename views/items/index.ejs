<!<%- include('../partials/header') %>

  <table id="list">
    <thead>
      <tr>
        <th>Items</th>
        <th>Owner</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% items.forEach(function(i) { %>
        <tr>
          <td>
            <%= i.name %>
          </td>
          <td>
            <%= i.owner %>
          </td>
          <td><a href="/items/<%= i._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
    </tbody>
  </table>

  <%- include('../partials/footer') %>