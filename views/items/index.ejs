<%- include('../partials/header') %>

  <h1>Item Inventory List</h1>
  <table>
    <thead>
      <tr>
        <th>Item</th>
        <th>Brand</th>
        <th>Owner</th>
      </tr>
    </thead>
    <tbody>
      <!-- Write the line of EJS to iterate over movies using forEach -->
      <% items.forEach(function(i) { %>
        <tr>
          <td><%= i.item %></td>
          <td><%= i.brand %></td>
          <td><%= i.owner %></td>
          <td><a href="/items/<%= i._id %>">DETAILS</a></td>         
        </tr>
      <!-- Close the forEach using EJS here -->
      <% }); %>
    </tbody>
  </table>

  <%- include('../partials/footer') %>